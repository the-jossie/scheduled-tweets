<h1>Sign In</h1>
<%= form_with url: login_path do |form| %>
  <div class="mb-3">
    <%= form.label :email %>
    <%= form.text_field :email, required: true,
 class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= form.label :password %>
    <%= form.password_field :password, required: true,
 class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= form.submit "Sign In", class: "btn btn-outline-primary" %>
  </div>
<% end%>
